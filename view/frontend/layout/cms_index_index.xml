<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <block class="Elie\InstagramFeed\Block\Feed" 
                   name="instagram.feed" 
                   template="Elie_InstagramFeed::feed.phtml"
                   after="-">
                <arguments>
                    <argument name="cache_lifetime" xsi:type="number">600</argument>
                    <argument name="cache_tags" xsi:type="array">
                        <item name="instagram_feed" xsi:type="string">RL_INSTAGRAM_FEED</item>
                    </argument>
                </arguments>
            </block>
        </referenceContainer>
    </body>
</page>
